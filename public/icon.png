<svg width="300" height="80" viewBox="0 0 300 80" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Gradient for the icon background -->
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4A7FFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2B5FE0;stop-opacity:1" />
    </linearGradient>
    
    <!-- Shadow filter -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="2"/>
      <feOffset dx="0" dy="2" result="offsetblur"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.3"/>
      </feComponentTransfer>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Icon background with rounded corners and gradient -->
  <rect x="10" y="10" width="60" height="60" rx="12" ry="12" fill="url(#iconGradient)" filter="url(#shadow)"/>
  
  <!-- House icon - enhanced with better proportions -->
  <g transform="translate(40, 40)">
    <!-- Roof -->
    <path d="M -12,-8 L 0,-16 L 12,-8 L 12,-5 L 10,-5 L 10,-7 L 0,-14 L -10,-7 L -10,-5 L -12,-5 Z" 
          fill="white" opacity="0.95"/>
    
    <!-- House body -->
    <rect x="-10" y="-5" width="20" height="16" fill="white" opacity="0.95" rx="1"/>
    
    <!-- Door -->
    <rect x="-3" y="3" width="6" height="8" fill="url(#iconGradient)" rx="1"/>
    
    <!-- Windows -->
    <rect x="-8" y="-1" width="4" height="4" fill="#4A7FFF" opacity="0.6" rx="0.5"/>
    <rect x="4" y="-1" width="4" height="4" fill="#4A7FFF" opacity="0.6" rx="0.5"/>
    
    <!-- Door knob -->
    <circle cx="1" cy="7" r="0.5" fill="white"/>
  </g>
  
  <!-- Text: AdminEstate -->
  <text x="85" y="48" font-family="'Arial', 'Helvetica', sans-serif" font-size="28" font-weight="700" fill="#1a1a1a" letter-spacing="-0.5">
    Admin<tspan fill="#2B5FE0">Estate</tspan>
  </text>
</svg>
